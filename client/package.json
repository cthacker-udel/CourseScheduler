{
    "name": "react-typescript-starter",
    "homepage": "/",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.1.1",
        "@fortawesome/free-solid-svg-icons": "^6.1.1",
        "@fortawesome/react-fontawesome": "^0.1.18",
        "@testing-library/jest-dom": "^5.16.1",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.4.0",
        "@types/node": "^16.11.21",
        "@types/react": "^17.0.38",
        "@types/react-dom": "^17.0.11",
        "modal": "^1.2.0",
        "react": "^17.0.2",
        "react-bootstrap": "^2.1.1",
        "react-csv": "^2.2.2",
        "react-dom": "^17.0.2",
        "react-hook-form": "^7.31.1",
        "react-intl": "^5.25.1",
        "react-modal": "^3.14.4",
        "react-router-dom": "^6.3.0",
        "react-scripts": "^5.0.1",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "test:cov": "react-scripts test --coverage --watchAll",
        "eject": "react-scripts eject",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "lint": "eslint ./src --ext .tsx --ext .ts --max-warnings 0",
        "format": "prettier --config .prettierrc --write src/**/*.{ts,tsx}",
        "extract-messages": "set NODE_ENV=production && babel ./src >NUL"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/cli": "^7.17.10",
        "@babel/core": "^7.17.10",
        "@types/react-csv": "^1.1.2",
        "@types/react-modal": "^3.13.1",
        "@typescript-eslint/eslint-plugin": "~5.17.0",
        "@typescript-eslint/parser": "~5.17.0",
        "babel-plugin-react-intl": "^8.2.25",
        "eslint": "~8.12.0",
        "eslint-config-intense": "^0.8.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-eslint-comments": "~3.2.0",
        "eslint-plugin-import": "~2.25.4",
        "eslint-plugin-jest": "~26.1.3",
        "eslint-plugin-jest-formatting": "~3.1.0",
        "eslint-plugin-no-secrets": "^0.8.9",
        "eslint-plugin-node": "~11.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-ramda": "~2.5.1",
        "eslint-plugin-react": "~7.29.4",
        "eslint-plugin-react-hooks": "~4.4.0",
        "eslint-plugin-simple-import-sort": "~7.0.0",
        "gh-pages": "^3.2.3",
        "prettier": "2.5.1",
        "prettier-eslint": "^13.0.0",
        "typescript": "~4.6.3"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.ts",
            "src/**/*.tsx",
            "!src/index.tsx",
            "!src/reportWebVitals.ts",
            "!src/react-app-env.d.ts"
        ]
    }
}
